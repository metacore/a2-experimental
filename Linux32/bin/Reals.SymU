	PROCEDURE {Fingerprint(-4591926361091339056)}  - Round*(x: REAL): LONGINT;
	CODE {SYSTEM.i386, SYSTEM.FPU}
	FLD DWORD [ESP]
	FISTP DWORD [ESP]	; store integer using current rounding mode
	FWAIT
	POP EAX	; return value

	END Round; 
	PROCEDURE {Fingerprint(3068067205373221981)}  - RoundL*(x: LONGREAL): LONGINT;
	CODE {SYSTEM.i386, SYSTEM.FPU}
	FLD QWORD [ESP]
	FISTP DWORD [ESP]	; store integer using current rounding mode
	FWAIT
	POP EAX	; return value
	POP EBX	; drop rest of parameter

	END RoundL; 
