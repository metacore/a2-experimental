<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<SyntaxHighlighter>
	<Styles>
		<Style name="Comment" fontname="Syntax" fontsize="10" fontstyle="{}" color="808080FF"/>
		<Style name="Control" fontname="Syntax" fontsize="10" fontstyle="{0}" color="80FF"/>
		<Style name="Modifiers" fontname="Syntax" fontsize="10" fontstyle="{}" color="FFFF00FF"/>
		<Style name="Locking" fontname="Syntax" fontsize="10" fontstyle="{}" color="FF00FFFF"/>
		<Style name="Checks" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FFFF"/>
		<Style name="Threads" fontname="Syntax" fontsize="10" fontstyle="{}" color="FF00FFFF"/>
		<Style name="Structure" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FF"/>
		<Style name="Operators" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FFFF"/>
		<Style name="Values" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FFFF"/>
		<Style name="BasicTypes" fontname="Syntax" fontsize="10" fontstyle="{}" color="FFFF"/>
		<Style name="ComplexTypes" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FFFF"/>
		<Style name="Lowlevel" fontname="Syntax" fontsize="10" fontstyle="{}" color="FF0000FF"/>
		<Style name="Dangerous" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FF0000FF"/>

		<Style name="Bold" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FF"/>
		<Style name="Italic" fontname="Syntax" fontsize="10" fontstyle="{1}" color="FF"/>
		<Style name="Red" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FF0000FF"/>
		<Style name="Blue" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FFFF"/>
		<Style name="Green" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FF00FF"/>

	</Styles>

	<Highlighters>
		<Highlighter name="Oberon">
			<Words allowCharacters="._">
				<Group style="Control">
					FOR TO WHILE DO REPEAT UNTIL IF THEN ELSE ELSIF
					LOOP EXIT CASE BY RETURN
					BEGIN END
				</Group>
				<Group style="Bold">
					NEW SELF
					SHORT LONG ENTIER ENTIERH
				</Group>
				<Group style="Bold">
					ABS ASH CAP CHR ORD
					COPY GETPROCEDURE LEN
					MAX MIN ODD SIZEO
				</Group>
				<Group style="Modifiers">
					DELEGATE
				</Group>
				<Group style="Locking">
					EXCLUSIVE
					*.AcquireRead
					*.ReleaseRead
					*.AcquireWrite
					*.ReleaseWrite
					*.Acquire
					*.Release
				</Group>
				<Group style="Checks">
					ASSERT
				</Group>
				<Group style="Threads">
					ACTIVE SHARED PRIORITY
					AWAIT
				</Group>
				<Group style="Structure">
					MODULE IMPORT TYPE DEFINITION REFINES IMPLEMENTS CONST VAR PROCEDURE
					VAR CONST
				</Group>
				<Group style="Operators">
					DIV MOD IN IS OR
					INCL EXCL
					INC DEC
				</Group>
				<Group style="Values">
					NIL TRUE FALSE
				</Group>
				<Group style="BasicTypes">
					BOOLEAN
					SHORTINT INTEGER LONGINT HUGEINT
					REAL LONGREAL
					SET
					CHAR
				</Group>
				<Group style="ComplexTypes">
					RECORD ARRAY POINTER OBJECT
				</Group>
				<Group style="Lowlevel">
					SYSTEM
					SYSTEM.GET SYSTEM.GET8 SYSTEM.GET16 SYSTEM.GET32 SYSTEM.GET64
					SYSTEM.ADDRESS SYSTEM.SIZE
					SYSTEM.SIZEOF SYSTEM.TYPEOF
				</Group>
				<Group style="Dangerous">
					UNTRACED
					SYSTEM.PUT SYSTEM.PUT8 SYSTEM.PUT16 SYSTEM.PUT32 SYSTEM.PUT64
					SYSTEM.VAL
					SYSTEM.ADR
					SYSTEM.MOVE
					HALT
					SYSTEM.PORTIN SYSTEM.PORTOUT
					SYSTEM.ROT SYSTEM.LSH
					SYSTEM.CLI SYSTEM.STI
				</Group>
			</Words>

			<Regions>
				<Region
					style="Comment" styleOpen="Comment" styleClose="Comment"
					open="(*" close="*)" nesting="TRUE" multiline="TRUE"
				/>
				<Region
					style="Values" styleOpen="Bold" styleClose="Bold"
					open='"' close='"' nesting="FALSE" multiline="FALSE"
				/>
				<Region
					style="Values" styleOpen="Bold" styleClose="Bold"
					open="'" close="'" nesting="FALSE" multiline="FALSE"
				/>
			</Regions>

		</Highlighter>

		<Highlighter name="XML">
			<Words>
				<Group style="Bold">
				</Group>
			</Words>
			<Regions>
				<Region
					style="Comment" styleOpen="Comment" styleClose="Comment"
					open="&lt;!--" close="-->" nesting="FALSE" multiline="TRUE"
				/>
				<Region
					style="Values" styleOpen="Bold" styleClose="Bold"
					open='"' close='"' nesting="FALSE" multiline="TRUE"
				/>
				<Region
					style="Values" styleOpen="Bold" styleClose="Bold"
					open="'" close="'" nesting="FALSE" multiline="TRUE"
				/>
			</Regions>
		</Highlighter>

		<Highlighter name="C">
			<Regions>
				<Region
					style="Comment" styleOpen="Comment" styleClose="Comment"
					open="/*" close="*/" nesting="FALSE" multiline="TRUE"
				/>
				<Region
					style="Comment" styleOpen="Comment" styleClose="Comment"
					open="//" close="" nesting="FALSE" multiline="FALSE"
				/>
			</Regions>
		</Highlighter>

		<Highlighter name="ReleaseTool">
			<Words allowCharacters=".">
				<Group style="Bold">
					HEADER VERSION END
					PACKAGE ARCHIVE SOURCE DESCRIPTION
					BUILDS
				</Group>
				<Group style="Italic">
					INCLUDE COMPILER COMPILEOPTIONS TARGET EXTENSION PATH EXCLUDEPACKAGES
				</Group>
				<Group style="Red">
					WIN iWIN eWIN
				</Group>
				<Group style="Blue">
					NATIVE AMD64 I386
				</Group>
				<Group style="Green">
					UNIX DARWIN LINUX SOLARIS
				</Group>
			</Words>
			<Regions>
				<Region
					style="Comment" styleOpen="Comment" styleClose="Comment"
					open="#" close="" nesting="FALSE" multiline="FALSE"
				/>
				<Region
					style="Values" styleOpen="Bold" styleClose="Bold"
					open='"' close='"' nesting="FALSE" multiline="FALSE"
				/>
			</Regions>
		</Highlighter>

		<Highlighter name="Assembler">
			<Words allowCharacters=":">
				<Group style="Bold">
					*:
					equ
				</Group>
			</Words>
			<Regions>
				<Region
					style="Comment" styleOpen="Comment" styleClose="Comment"
					open =";" close="" nesting="FALSE" multiline="FALSE"
				/>
			</Regions>
		</Highlighter>

	</Highlighters>

</SyntaxHighlighter>