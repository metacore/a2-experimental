<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

	<!DOCTYPE SyntaxHighlighter [
		<!ELEMENT SyntaxHighlighter (Styles, Highlighters)>
		<!ELEMENT Styles (Style*)>
		<!ELEMENT Style EMPTY>
		<!ELEMENT Highlighters (Highlighter*)>
		<!ELEMENT Highlighter (Words?, Regions?)>
		<!ELEMENT Words (#PCDATA)>
		<!ELEMENT Regions (Region*)>
		<!ELEMENT Region EMPTY>
		<!ATTLIST Style
			name CDATA #REQUIRED
			fontname CDATA #REQUIRED
			fontsize CDATA #REQUIRED
			fontstyle CDATA #REQUIRED
			color CDATA #REQUIRED
			bgcolor CDATA #IMPLIED
			voff CDATA #IMPLIED
		>
		<!ATTLIST Highlighter
			name CDATA #REQUIRED
			defaultstyle CDATA #IMPLIED>
		<!ATTLIST Words allowCharacters CDATA #IMPLIED>
		<!ATTLIST Group name CDATA #REQUIRED>
		<!ATTLIST Region
			style CDATA #REQUIRED
			styleOpen CDATA #REQUIRED
			styleClose CDATA #REQUIRED
			open CDATA #REQUIRED
			close CDATA #REQUIRED
			nesting CDATA #REQUIRED
			multiline CDATA #REQUIRED
		>
	]>

<SyntaxHighlighter>
	<Styles>
		<Style name="Comment" fontname="Syntax" fontsize="10" fontstyle="{}" color="808080FF"/>
		<Style name="CommentItalic" fontname="Syntax" fontsize="10" fontstyle="{1}" color="808080FF"/>
		<Style name="Control" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FFFF"/>
		<Style name="Modifiers" fontname="Syntax" fontsize="10" fontstyle="{}" color="A000FF"/>
		<Style name="Locking" fontname="Syntax" fontsize="10" fontstyle="{}" color="FF00FFFF"/>
		<Style name="Checks" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FFFF"/>
		<Style name="Threads" fontname="Syntax" fontsize="10" fontstyle="{}" color="FF00FFFF"/>
		<Style name="Structure" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FF"/>
		<Style name="Operators" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FFFF"/>
		<Style name="Values" fontname="Syntax" fontsize="10" fontstyle="{}" color="30A0A0FF"/>
		<Style name="BasicTypes" fontname="Syntax" fontsize="10" fontstyle="{}" color="FFFF"/>
		<Style name="ComplexTypes" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FFFF"/>
		<Style name="Dangerous" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FF0000FF"/>
		<Style name="Debug" fontname="Syntax" fontsize="10" fontstyle="{}" color="FFFF"/>
		<Style name="String" fontname="Syntax" fontsize="10" fontstyle="{}" color="BF003FFF"/>

		<Style name="Normal" fontname="Syntax" fontsize="10" fontstyle="{}" color="FF"/>
		<Style name="Bold" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FF"/>
		<Style name="Italic" fontname="Syntax" fontsize="10" fontstyle="{1}" color="FF"/>
		<Style name="Red" fontname="Syntax" fontsize="10" fontstyle="{}" color="FF0000FF"/>
		<Style name="Blue" fontname="Syntax" fontsize="10" fontstyle="{}" color="FFFF"/>
		<Style name="Green" fontname="Syntax" fontsize="10" fontstyle="{0}" color="9000FF"/>
	</Styles>

	<Highlighters>
		<Highlighter name="Oberon" defaultstyle="">
			<Tokens>
				<Group style="Green">
					( (* *) )
				</Group>
				<Group style="Red">
					= := * + - /
					&lt; &lt;= &gt;= &gt; #
				</Group>
				<Group style="Comment">
					; : ,
				</Group>
			</Tokens>

			<Words allowCharacters="._" numberStyle="Values">
				<Group style="Bold">
					FOR TO WHILE DO REPEAT UNTIL IF THEN ELSE ELSIF
					LOOP CASE BY
					BEGIN END CODE FINALLY
					OF
				</Group>
				<Group style="Green">
					RETURN EXIT
				</Group>
				<Group style="Bold">
					VAR CONST
					ARRAY POINTER
				</Group>
				<Group style="Bold">
					NEW SELF
				</Group>
				<Group style="Italic">
					ABS ASH CAP CHR ORD
					COPY GETPROCEDURE LEN
					MAX MIN ODD SIZEOF SHORT LONG ENTIER ENTIERH
					DIV MOD IN IS OR
					INCL EXCL INC DEC
				</Group>
				<Group style="Modifiers">
					DELEGATE
				</Group>
				<Group style="Locking">
					EXCLUSIVE
					Acquire Release
					*.AcquireRead 	*.ReleaseRead
					*.AcquireWrite *.ReleaseWrite
					*.Acquire *.Release
				</Group>
				<Group style="Checks">
					ASSERT
				</Group>
				<Group style="Threads">
					ACTIVE SHARED PRIORITY AWAIT
				</Group>
				<Group style="Structure">
					MODULE IMPORT TYPE DEFINITION REFINES IMPLEMENTS PROCEDURE
					OBJECT RECORD
				</Group>
				<Group style="Values">
					NIL TRUE FALSE
				</Group>
				<Group style="Bold">
					BOOLEAN
					SHORTINT INTEGER LONGINT HUGEINT
					REAL LONGREAL
					SET
					CHAR
					ANY
				</Group>
				<Group style="Dangerous">
					SYSTEM.GET SYSTEM.GET8 SYSTEM.GET16 SYSTEM.GET32 SYSTEM.GET64
					SYSTEM.ADDRESS SYSTEM.SIZE
					SYSTEM.SIZEOF SYSTEM.TYPEOF
					SYSTEM.PUT SYSTEM.PUT8 SYSTEM.PUT16 SYSTEM.PUT32 SYSTEM.PUT64
					SYSTEM.VAL
					SYSTEM.ADR
					SYSTEM.MOVE
					SYSTEM.PORTIN SYSTEM.PORTOUT
					SYSTEM.ROT SYSTEM.LSH
					SYSTEM.CLI SYSTEM.STI
					SYSTEM
					HALT
					UNTRACED
				</Group>
				<Group style="Debug">
					KernelLog
					KernelLog.*
				</Group>
			</Words>

			<Regions>
				<Region
					style="Comment" styleOpen="Comment" styleClose="Comment"
					open="(*" close="*)" nesting="TRUE" multiline="TRUE"
				/>
				<Region
					style="Blue" styleOpen="Comment" styleClose="Comment"
					open="(*!" close="*)" nesting="TRUE" multiline="TRUE"
				/>
				<Region
					style="Red" styleOpen="Comment" styleClose="Comment"
					open="(*?" close="*)" nesting="TRUE" multiline="TRUE"
				/>
				<Region
					style="Comment" styleOpen="CommentItalic" styleClose="CommentItalic"
					open="(**" close="*)" nesting="TRUE" multiline="TRUE"
				/>
				<Region
					style="String" styleOpen="Bold" styleClose="Bold"
					open='"' close='"' nesting="FALSE" multiline="FALSE"
				/>
				<Region
					style="String" styleOpen="Bold" styleClose="Bold"
					open="'" close="'" nesting="FALSE" multiline="FALSE"
				/>
			</Regions>

		</Highlighter>

		<Highlighter name="XML">
			<Words allowCharacters="?&amp;!#;:">
				<Group style="Bold">
					?xml
					!DOCTYPE !ELEMENT !ATTLIST
				</Group>
				<Group style="Italic">
					#DEFAULT #FIXED
					#REQUIRED #IMPLIED
					#PCDATA #CDATA
				</Group>
				<Group style="Bold">
					EMPTY ANY CDATA PCDATA
					ID IDREF IDREFS NMTOKEN NMTOKENS ENTITY ENTITIES NOTATION
					xml:*
				</Group>
			</Words>
			<Regions>
				<Region
					style="Comment" styleOpen="Comment" styleClose="Comment"
					open="&lt;!--" close="-->" nesting="FALSE" multiline="TRUE"
				/>
				<Region
					style="String" styleOpen="Bold" styleClose="Bold"
					open='"' close='"' nesting="FALSE" multiline="TRUE"
				/>
				<Region
					style="String" styleOpen="Bold" styleClose="Bold"
					open="'" close="'" nesting="FALSE" multiline="TRUE"
				/>
			</Regions>
		</Highlighter>

		<Highlighter name="C">
			<Regions>
				<Region
					style="Comment" styleOpen="Comment" styleClose="Comment"
					open="/*" close="*/" nesting="FALSE" multiline="TRUE"
				/>
				<Region
					style="Comment" styleOpen="Comment" styleClose="Comment"
					open="//" close="" nesting="FALSE" multiline="FALSE"
				/>
			</Regions>
		</Highlighter>

		<Highlighter name="ReleaseTool">
			<Words allowCharacters=".">
				<Group style="Bold">
					HEADER VERSION END
					PACKAGE ARCHIVE SOURCE DESCRIPTION
					BUILDS
				</Group>
				<Group style="Italic">
					INCLUDE COMPILER COMPILEOPTIONS TARGET EXTENSION PATH EXCLUDEPACKAGES DISABLED
				</Group>
				<Group style="Red">
					WIN iWIN eWIN
				</Group>
				<Group style="Blue">
					NATIVE AMD64 I386
				</Group>
				<Group style="Green">
					UNIX DARWIN LINUX SOLARIS
				</Group>
			</Words>
			<Regions>
				<Region
					style="Comment" styleOpen="Comment" styleClose="Comment"
					open="#" close="" nesting="FALSE" multiline="FALSE"
				/>
				<Region
					style="String" styleOpen="Bold" styleClose="Bold"
					open='"' close='"' nesting="FALSE" multiline="FALSE"
				/>
			</Regions>
		</Highlighter>

		<Highlighter name="Assembler">
			<Words allowCharacters=":">
				<Group style="Bold">
					*:
				</Group>
			</Words>
			<Regions>
				<Region
					style="Comment" styleOpen="Comment" styleClose="Comment"
					open =";" close="" nesting="FALSE" multiline="FALSE"
				/>
			</Regions>
		</Highlighter>

	</Highlighters>

</SyntaxHighlighter>