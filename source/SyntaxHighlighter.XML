<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<SyntaxHighlighter>
	<Styles>
		<Style name="Comment" fontname="Syntax" fontsize="10" fontstyle="{}" color="808080FF" bgcolor="0" voff="0"/>
		<Style name="Control" fontname="Syntax" fontsize="10" fontstyle="{0}" color="80FF" bgcolor="0" voff="0"/>
		<Style name="Modifiers" fontname="Syntax" fontsize="10" fontstyle="{}" color="FFFF00FF" bgcolor="FFFFFFFF" voff="0"/>
		<Style name="Locking" fontname="Syntax" fontsize="10" fontstyle="{}" color="FF00FFFF" bgcolor="0" voff="0"/>
		<Style name="Checks" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FFFF" bgcolor="0" voff="0"/>
		<Style name="Threads" fontname="Syntax" fontsize="10" fontstyle="{}" color="FF00FFFF" bgcolor="0" voff="0"/>
		<Style name="Structure" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FF" bgcolor="0" voff="0"/>
		<Style name="Operators" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FFFF" bgcolor="0" voff="0"/>
		<Style name="Values" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FFFF" bgcolor="0" voff="0"/>
		<Style name="BasicTypes" fontname="Syntax" fontsize="10" fontstyle="{}" color="FFFF" bgcolor="0" voff="0"/>
		<Style name="ComplexTypes" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FFFF" bgcolor="0" voff="0"/>
		<Style name="Lowlevel" fontname="Syntax" fontsize="10" fontstyle="{}" color="FF0000FF" bgcolor="0" voff="0"/>
		<Style name="Dangerous" fontname="Syntax" fontsize="10" fontstyle="{0}" color="FF0000FF" bgcolor="0" voff="0"/>
	</Styles>

	<Highlighters>
		<Highlighter name="Oberon">
			<Words>
				<Group style="Control">
					FOR TO WHILE DO REPEAT UNTIL IF THEN ELSE ELSIF
					LOOP EXIT CASE BY RETURN
					BEGIN END
				</Group>
				<Group style="Modifiers">
					DELEGATE
				</Group>
				<Group style="Locking">
					EXCLUSIVE
				</Group>
				<Group style="Checks">
					ASSERT
				</Group>
				<Group style="Threads">
					ACTIVE SHARED PRIORITY
				</Group>
				<Group style="Structure">
					MODULE IMPORT TYPE DEFINITION REFINES CONST VAR PROCEDURE
					VAR CONST
				</Group>
				<Group style="Operators">
					DIV MOD IN IS OR
				</Group>
				<Group style="Values">
					NIL TRUE FALSE
				</Group>
				<Group style="BasicTypes">
					BOOLEAN
					SHORTINT INTEGER LONGINT
					REAL LONGREAL
					SET
					CHAR
				</Group>
				<Group style="ComplexTypes">
					RECORD ARRAY POINTER OBJECT
				</Group>
				<Group style="Lowlevel">
					SYSTEM
					SYSTEM.GET SYSTEM.GET8 SYSTEM.GET16 SYSTEM.GET32
					SYSTEM.ADDRESS SYSTEM.SIZE
					SYSTEM.SIZEOF SYSTEM.TYPEOF
				</Group>
				<Group style="Dangerous">
					UNTRACED
					SYSTEM.PUT SYSTEM.PUT8 SYSTEM.PUT16 SYSTEM.PUT32
					SYSTEM.VAL
					SYSTEM.ADR
					HALT
				</Group>
			</Words>

			<Regions>
				<Region
					style="Comment"
					open="(*" close="*)" nesting="TRUE" multiline="TRUE"
				/>
			</Regions>

		</Highlighter>

	</Highlighters>

</SyntaxHighlighter>