positive: Gaussian natural sum using arrays

	MODULE Test;
	CONST Count = 100;
	VAR i, sum: LONGINT; a: ARRAY Count OF LONGINT;
	BEGIN
		FOR i := 0 TO Count - 1 DO
			a[i] := i;
		END;
		i := 0; sum := 0;
		WHILE i < Count DO
			INC (sum, a[i]); INC (i);
		END;
		ASSERT (sum = Count * (Count-1) DIV 2);
	END Test.

