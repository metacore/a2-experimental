MODULE LayLaS IN Oberon;
	IMPORT Texts, Oberon, Layouts;
CONST 
	null* = 0; 
	open* = 1; 
	close* = 2; 
	equal* = 4; 
	lbrak* = 6; 
	rbrak* = 7; 
	String* = 20; 
	Int* = 21; 
	Real* = 22; 
	Char* = 23; 
	Bool* = 24; 
	Qualident* = 28; 
	Ident* = 29; 
	Config* = 30; 
	New* = 31; 
	HList* = 32; 
	VList* = 33; 
	Table* = 34; 
	Place* = 35; 
	Deep* = 37; 
	Shallow* = 38; 
	Include* = 39; 
	Def* = 40; 
	Set* = 41; 
	Params* = 42; 
	Layout* = 43; 
	Attributes* = 44; 
	Links* = 45; 
	Virtual* = 46; 
	Default* = 47; 
	Break* = 48; 
	Span* = 49; 
	Left* = Layouts.Left; 
	Right* = Layouts.Right; 
	Bottom* = Layouts.Bottom; 
	Top* = Layouts.Top; 
	Center* = Layouts.Center; 
