	Const3 = 52; 
	RM = 64; 
	RM8 = 65; 
	RM16 = 66; 
	RM32 = 67; 
	MM = 68; 
	MM32 = 69; 
	MM64 = 70; 
	XMM = 160; 
	XMM32 = 161; 
	XMM64 = 162; 
	XMM128 = 163; 
	M = 72; 
	M8 = 73; 
	M16 = 74; 
	M32 = 75; 
	M64 = 76; 
	M80 = 77; 
	M128 = 78; 
	SReg = 80; 
	RegCS = 81; 
