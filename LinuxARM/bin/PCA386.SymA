	RegSS = 86; 
	Rel8 = 96; 
	Rel16 = 97; 
	Rel32 = 98; 
	FwdJmp = 99; 
	CRReg = 112; 
	DRReg = 113; 
	TRReg = 114; 
	FReg = 128; 
	FRegST = 129; 
	TAB = 9X; 
	LF = 0AX; 
	CR = 0DX; 
	SPACE = 20X; 
	ScUndef = 0; 
	ScIdent = 1; 
	ScSemicolon = 2; 
	ScAt = 3; 
	ScNumber = 4; 
	ScComma = 5; 
	ScLBrak = 6; 
	ScRBrak = 7; 
