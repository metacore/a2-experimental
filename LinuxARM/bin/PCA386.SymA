	none =  -1; 
	i386 = 0; 
	i486 = 1; 
	Pentium = 2; 
	PentiumPro = 3; 
	FPU = 16; 
	Privileged = 17; 
	MMX = 18; 
	SSE = 19; 
	SSE2 = 20; 
	SSE3 = 21; 
	SSE4 = 22; 
	StLabel = 0; 
	StFwdLabel = 1; 
	StConst = 3; 
	StType = 4; 
	StVar = 5; 
	StVarPar = 6; 
	StGlobal = 7; 
	StMod = 11; 
	Reg8 = 8; 
	Reg16 = 16; 
	Reg32 = 32; 
	MReg = 40; 
	XReg = 41; 
	RegAL = 9; 
	RegAX = 17; 
	RegEAX = 33; 
	RegDX = 18; 
	RegCL = 19; 
	Imm8 = 48; 
	Imm16 = 49; 
	Imm32 = 50; 
	SImm8 = 53; 
	Imm = 54; 
