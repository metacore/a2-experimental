	PROCEDURE ^ cos*(x: Value): Value; 
	PROCEDURE ^ arctan*(x: Value): Value; 
	PROCEDURE ^ sqrt*(x: Value): Value; 
	PROCEDURE ^ ln*(x: Value): Value; 
	PROCEDURE ^ exp*(x: Value): Value; 
BEGIN
END Math.
