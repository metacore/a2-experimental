MODULE Math;
	IMPORT SYSTEM;
CONST 
	e* = 2.718281828459046; 
	pi* = 3.141592653589793; 
	ln2* = 6.931471805599454E-1; 
	eps = 1.2E-7; 
TYPE 
	Value = FLOAT32; 

	PROCEDURE ^ Expo(x: Value): UNSIGNED32; 
	PROCEDURE ^ Mantissa(x: Value): UNSIGNED32; 
	PROCEDURE ^ Equal(x, y: Value): BOOLEAN; 
