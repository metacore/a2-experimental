	universe, syslink: Object; 
	strucForm: ARRAY (SFtypEnhArray + 1) OF SHORTINT; 
	strucComp: ARRAY (SFtypEnhArray + 1) OF SHORTINT; 
	predefStruct: ARRAY (SFlastStruct + 1) OF Struct; 
	strucFormSF: ARRAY (Comp + 1) OF SHORTINT; 
	strucCompSF: ARRAY (Comp + 1) OF SHORTINT; 
	export: BOOLEAN; 
	exp: ARRAY (Operator + 1) OF Object; 
