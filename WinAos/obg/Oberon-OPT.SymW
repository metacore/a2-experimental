	Const* = POINTER TO ConstDesc; 

	Object* = POINTER TO ObjDesc; 

	Module* = POINTER TO ModDesc; 

	Struct* = POINTER TO StrDesc; 

	Node* = POINTER TO NodeDesc; 

	ConstExt* = POINTER TO OPS.String; 

	ReadStringProc = PROCEDURE (VAR R: Files.Rider; VAR string: ARRAY OF CHAR); 

	ConstDesc* = RECORD 
		ext*: ConstExt; 
		intval*: LONGINT; 
		intval2*: LONGINT; 
		setval*: SET; 
		realval*: LONGREAL; 
	END; 

	ObjDesc* = RECORD 
		left*, right*, link*, scope*: Object; 
		myscope*: Object; 
		link2*, old: Object; 
		prio*, sysflag*: SHORTINT; 
		code*: Node; 
		name*: OPS.Name; 
		leaf*: BOOLEAN; 
		mode*: SHORTINT; 
		mnolev*: LONGINT; 
		vis*: SHORTINT; 
		typ*: Struct; 
		conval*: Const; 
		adr*, linkadr*: LONGINT; 
		nxtobj*, sibling*: Object; 
		par: BOOLEAN; 
		inited*, used*: BOOLEAN; 
		fp*: LONGINT; 
		history*: SHORTINT; 
		txtpos*: LONGINT; 
		flag*: SET; 
		finallyOff*: LONGINT; 
	END; 

	StructArr = POINTER TO ARRAY OF Struct; 

	ModArr = POINTER TO ARRAY OF Module; 

	ModDesc* = RECORD (ObjDesc)
		publics*: Object; 
		directImp*: BOOLEAN; 
		nofimp: INTEGER; 
		import: ModArr; 
		nofstr: INTEGER; 
		struct: StructArr; 
		nofreimp: INTEGER; 
		reimp: StructArr; 
	END; 

	StrDesc* = RECORD 
		form*, comp*, extlev*: SHORTINT; 
		mno*: LONGINT; 
		ref*: INTEGER; 
		align*, sysflag*: SHORTINT; 
		rectest*, n*, size*, tdadr*, offset*, txtpos*, txtpos2*: LONGINT; 
