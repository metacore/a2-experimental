		link*, strobj*: Object; 
		fpdone*, sref*, oref*: INTEGER; 
		pvused*, pbused*: BOOLEAN; 
		fp*, pvfp*, pbfp*: LONGINT; 
		incomplete*: BOOLEAN; 
		ptr*: Struct; 
	END; 

	NodeDesc* = RECORD 
		left*, right*, link*: Node; 
		class*, subcl*: SHORTINT; 
		readonly*: BOOLEAN; 
		typ*: Struct; 
		obj*: Object; 
		conval*: Const; 
		global*: BOOLEAN; 
		up*, down*: Node; 
		ref*: LONGINT; 
		flag*, subflg*: SHORTINT; 
	END; 
