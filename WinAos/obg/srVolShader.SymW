MODULE srVolShader;
	IMPORT srBase;
TYPE 
	SREAL = srBase.SREAL; 

	Color = srBase.COLOR; 

	Shader* = OBJECT 

		PROCEDURE ^ Shade*(VAR x, y, z, r, g, b, a: SREAL); 
	END Shader; 

	checkerboard3d* = OBJECT (Shader)
	VAR 
		isquish*, jsquish*, ksquish*: INTEGER; 
		black, white: Color; 

		PROCEDURE ^ squish*(i, j, k: INTEGER); 
		PROCEDURE ^ set*(b, w: Color); 
		PROCEDURE ^ Shade*(VAR x, y, z, r, g, b, a: SREAL); 
	END checkerboard3d; 

BEGIN
END srVolShader.
